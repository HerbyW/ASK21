<?xml version="1.0"?>
<!--from the bocian by AJ MacLeod-->
<PropertyList>
  <fx>
    
    <!-- Motor start -->
    
    <crank>
      <name>engstart</name>
      <path>Sounds/engstart.wav</path>
      <property>sim/multiplay/generic/int[13]</property>
      <volume>
	<offset>0.25</offset>
      </volume>
    </crank>
    
    <crank>
      <name>crank</name>
      <mode>looped</mode>
      <path>Sounds/cranking.wav</path>
      <condition>
	<property>sim/multiplay/generic/int[13]</property>
	<not>
	  <property>sim/multiplay/generic/int[12]</property>
	</not>
      </condition>
      <volume>
	<offset>0.25</offset>
      </volume>
    </crank>
    
    <crank>
      <name>cough</name>
      <path>Sounds/coughing.wav</path>
      <property>sim/multiplay/generic/int[12]</property>
      <volume>
	<offset>0.25</offset>
      </volume>
    </crank>
    
    <engine>
      <name>engine</name>
      <mode>looped</mode>
      <path>Sounds/wasp.wav</path>
      <property>sim/multiplay/generic/int[12]</property>
      <volume>
	<property>sim/multiplay/generic/float[12]</property>
	<factor>0.00375</factor>
	<min>0.15</min>
	<max>0.5</max>
      </volume>
      <volume>
	<property>sim/multiplay/generic/float[13]</property>
	<type>log</type>
	<min>1.0</min>
	<max>1.5</max>
      </volume>
      <volume>
	<property>/sim/current-view/internal</property>
	<offset>1.0</offset>
	<factor>-0.7</factor>
      </volume>
      <pitch>
	<property>/engines/engine[0]/rpm</property>
	<factor>0.0004</factor>
	<min>0.5</min>
	<max>2.0</max>
	<offset>0.15</offset>
      </pitch>
    </engine>
    
    <propeller>
      <name>propeller</name>
      <mode>looped</mode>
      <path>Sounds/turboprop1.wav</path>
      <property>sim/multiplay/generic/int[12]</property>
      <volume>
	<property>sim/multiplay/generic/float[13]</property>
	<type>log</type>
	<min>0.2</min>
	<max>1.0</max>
      </volume>
      <volume>
	<property>/sim/current-view/internal</property>
	<factor>0.5</factor>
      </volume>
      <pitch>
	<property>sim/multiplay/generic/float[12]</property>
	<factor>0.0036</factor>
	<min>0.5</min>
	<max>2.0</max>
	<offset>0.9</offset>
      </pitch>
    </propeller>
    
    <click> 
      <name>Magnetos-off</name>
      <path>Sounds/click.wav</path>
      <condition>
	<not>
	  <property>controls/engines/engine[0]/magnetos</property>
	</not>
      </condition>
      <volume> 
	<factor>0.8</factor>
      </volume>
    </click>
    
    <click> 
      <name>Magnetos-R</name>
      <path>Sounds/click.wav</path>
      <condition>
	<equals>
	  <property>controls/engines/engine[0]/magnetos</property>
	  <value>1</value>
	</equals>
      </condition>
      <volume> 
	<factor>0.8</factor>
      </volume>
    </click>
    
    <click> 
      <name>Magnetos-L</name>
      <path>Sounds/click.wav</path>
      <condition>
	<equals>
	  <property>controls/engines/engine[0]/magnetos</property>
	  <value>2</value>
	</equals>
      </condition>
      <volume> 
	<factor>0.8</factor>
      </volume>
    </click>
    
    <click> 
      <name>Magnetos-B</name>
      <path>Sounds/click.wav</path>
      <condition>
	<equals>
	  <property>controls/engines/engine[0]/magnetos</property>
	  <value>3</value>
	</equals>
      </condition>
      <volume> 
	<factor>0.8</factor>
      </volume>
    </click>
    
    <!-- Motor end -->
    
    <nose-wheel-spin>
      <name>wheel-spin</name>
      <mode>looped</mode>
      <path>Aircraft/ASK21/Sounds/wheelspin-loop.wav</path>
      <volume>
	<property>/gear/gear[0]/rollspeed-ms</property>
	<factor>0.02</factor>
      </volume>
      <pitch>
	<property>/gear/gear[0]/rollspeed-ms</property>
	<factor>0.01</factor>
	<offset>1</offset>
      </pitch>
      <position>
	<x>-3.2</x>
	<y>0.0</y>
	<z>0.2</z>
      </position>
      <reference-dist>2</reference-dist>
      <max-dist>10</max-dist>
    </nose-wheel-spin>
    
    <main-wheel-spin>
      <name>wheel-spin</name>
      <mode>looped</mode>
      <path>Aircraft/ASK21/Sounds/wheelspin-loop.wav</path>
      <volume>
	<property>/gear/gear[1]/rollspeed-ms</property>
	<factor>0.02</factor>
      </volume>
      <pitch>
	<property>/gear/gear[1]/rollspeed-ms</property>
	<factor>0.006</factor>
	<offset>1</offset>
      </pitch>
      <position>
	<x>-1</x>
	<y>0.0</y>
	<z>0.2</z>
      </position>
      <reference-dist>2</reference-dist>
      <max-dist>10</max-dist>
    </main-wheel-spin>
    
    <wheel-rumble>
      <name>wheel-rumble-front</name>
      <mode>looped</mode>
      <path>Aircraft/ASK21/Sounds/ground_roll3.wav</path>
      <condition>
	<or>
	  <property>/gear/gear[0]/wow</property>
	</or>
      </condition>
      <volume>
	<property>velocities/groundspeed-kt</property>
	<factor>0.05</factor>
      </volume>
      <position>
	<x>-3.2</x>
	<y>0.0</y>
	<z>0.2</z>
      </position>
      <reference-dist>5</reference-dist>
      <max-dist>30</max-dist>
    </wheel-rumble>
    
    <wheel-rumble>
      <name>wheel-rumble-main</name>
      <mode>looped</mode>
      <path>Aircraft/ASK21/Sounds/ground_roll3.wav</path>
      <condition>
	<or>
	  <property>/gear/gear[1]/wow</property>
	</or>
      </condition>
      <volume>
	<property>velocities/groundspeed-kt</property>
	<factor>0.05</factor>
      </volume>
      <position>
	<x>-1</x>
	<y>0.0</y>
	<z>0.2</z>
      </position>
      <reference-dist>5</reference-dist>
      <max-dist>30</max-dist>
    </wheel-rumble>
    
    <wheel-rumble>
      <name>wheel-rumble-rear</name>
      <mode>looped</mode>
      <path>Aircraft/ASK21/Sounds/ground_roll3.wav</path>
      <condition>
	<or>
	  <property>/gear/gear[2]/wow</property>
	</or>
      </condition>
      <volume>
	<property>velocities/groundspeed-kt</property>
	<factor>0.05</factor>
      </volume>
      <position>
	<x>3.6</x>
	<y>0.0</y>
	<z>0.5</z>
      </position>
      <reference-dist>5</reference-dist>
      <max-dist>30</max-dist>
    </wheel-rumble>
    
    <wheelbrake>
      <name>wheelbrake</name>
      <mode>looped</mode>
      <path>Aircraft/ASK21/Sounds/wheelbrake.wav</path>
      <condition>
	<and>
	  <property>/gear/gear[1]/wow</property>
	  <greater-than>
	    <property>/surface-positions/speedbrake-pos-norm</property>
	    <value>0.85</value>
	  </greater-than>
	</and>
      </condition>
      <volume>
	<property>velocities/groundspeed-kt</property>
	<factor>0.05</factor>
      </volume>
    </wheelbrake>
    <!--Wind outside-->
    <wind>
      <name>wind</name>
      <mode>looped</mode>
      <path>Sounds/wind.wav</path>
      <property>velocities/airspeed-kt</property>
      <volume>
	<property>position/altitude-ft</property>
	<factor>-0.000015</factor>
	<offset>1.0</offset>
	<min>0.1</min>
	<max>1.0</max>
      </volume>
      <volume>
	<property>velocities/airspeed-kt</property>
	<factor>0.005</factor>
	<min>0.0</min>
	<max>1</max>
	<offset>-0.2</offset>
      </volume>
      <pitch>
	<property>velocities/airspeed-kt</property>
	<factor>0.01</factor>
	<offset>1</offset>
      </pitch>
    </wind>
    <!--Wind in cockpit - front-->
    <wind>
      <name>wind front</name>
      <mode>looped</mode>
      <path>Sounds/wind.wav</path>
      <property>velocities/airspeed-kt</property>
      <volume>
	<property>position/altitude-ft</property>
	<factor>-0.000015</factor>
	<offset>1.0</offset>
	<min>0.1</min>
	<max>1.0</max>
      </volume>
      <volume>
	<property>velocities/airspeed-kt</property>
	<factor>0.01</factor>
	<min>0.0</min>
	<max>5</max>
	<offset>-0.2</offset>
      </volume>
      <volume>
	<property>sim/model/door-positions/frontwin/position-norm</property>
	<interpolation>
	  <entry>
	    <ind>0.0</ind>
	    <dep>0.25</dep>
	  </entry>
	  <entry>
	    <ind>0.1</ind>
	    <dep>0.65</dep>
	  </entry>
	  <entry>
	    <ind>1</ind>
	    <dep>0.75</dep>
	  </entry>
	</interpolation><!--
	<factor>0.95</factor>
	<min>0.05</min>
	<max>1</max>
	<offset>0.05</offset>-->
      </volume>
      <pitch>
	<property>velocities/airspeed-kt</property>
	<factor>0.01</factor>
	<offset>1.1</offset>
      </pitch>
      <position>
	<x>-2.8</x>
	<y>0.0</y>
	<z>0.8</z>
      </position>
      <reference-dist>0.5</reference-dist>
      <max-dist>2</max-dist>
    </wind>
    <!--Wind in cockpit - rear-->
    <wind>
      <name>wind rear</name>
      <mode>looped</mode>
      <path>Sounds/wind.wav</path>
      <property>velocities/airspeed-kt</property>
      <volume>
	<property>position/altitude-ft</property>
	<factor>-0.000015</factor>
	<offset>1.0</offset>
	<min>0.1</min>
	<max>1.0</max>
      </volume>
      <volume>
	<property>velocities/airspeed-kt</property>
	<factor>0.01</factor>
	<min>0.03</min>
	<max>5</max>
	<offset>-0.2</offset>
      </volume>
      <volume>
	<property>sim/model/door-positions/rearwin/position-norm</property>
	<interpolation>
	  <entry>
	    <ind>0.0</ind>
	    <dep>0.25</dep>
	  </entry>
	  <entry>
	    <ind>0.1</ind>
	    <dep>0.65</dep>
	  </entry>
	  <entry>
	    <ind>1</ind>
	    <dep>0.75</dep>
	  </entry>
	</interpolation><!--
	<factor>0.95</factor>
	<min>0.05</min>
	<max>1</max>
	<offset>0.05</offset>-->
      </volume>
      <pitch>
	<property>velocities/airspeed-kt</property>
	<factor>0.01</factor>
	<offset>1.05</offset>
      </pitch>
      <position>
	<x>-2</x>
	<y>0.0</y>
	<z>0.9</z>
      </position>
      <reference-dist>0.5</reference-dist>
      <max-dist>2</max-dist>
    </wind>
    
    <airbrakes>
      <name>airbrakes</name>
      <mode>looped</mode>
      <path>Aircraft/ASK21/Sounds/airbrakesL.wav</path>
      <property>velocities/airspeed-kt</property>
      <volume>
	<property>/surface-positions/speedbrake-pos-norm</property>
	<factor>0.6</factor>
	<offset>0.0</offset>
	<min>0.0</min>
	<max>0.5</max>
      </volume>
      <volume>
	<property>velocities/airspeed-kt</property>
	<factor>0.015</factor>
	<min>0.00</min>
	<max>0.5</max>
	<offset>-0.1</offset>
      </volume>
      <pitch>
	<property>velocities/airspeed-kt</property>
	<factor>0.002</factor>
	<offset>1</offset>
      </pitch>
    </airbrakes>
    
    <airbrake-open>
      <name>airbrake-open</name>
      <mode>once</mode>
      <path>Aircraft/ASK21/Sounds/airbrake-open.wav</path>
      <condition>
	<greater-than>
	  <property>/surface-positions/speedbrake-pos-norm</property>
	  <value>0.06</value>
	</greater-than>
      </condition>
      <position>
	<x>-1.2</x>
	<y>0.0</y>
	<z>1</z>
      </position>
      <reference-dist>2</reference-dist>
      <max-dist>10</max-dist>
    </airbrake-open>
    
    <airbrake-close>
      <name>airbrake-close</name>
      <mode>once</mode>
      <path>Aircraft/ASK21/Sounds/airbrake-close.wav</path>
      <condition>
	<less-than>
	  <property>/surface-positions/speedbrake-pos-norm</property>
	  <value>0.04</value>
	</less-than>
      </condition>
      <position>
	<x>-1.2</x>
	<y>0.0</y>
	<z>1</z>
      </position>
      <reference-dist>2</reference-dist>
      <max-dist>10</max-dist>
    </airbrake-close>
    
    <variometer-up>
      <name>variometer-up</name>
      <mode>looped</mode>
      <path>Aircraft/ASK21/Sounds/vario.wav</path>
      <condition>
	<and>
	  <greater-than>
	    <property>/instrumentation/variometer/te-reading-mps</property>
	    <value>0.02</value>
	  </greater-than>
	  <property>/controls/electric/battery-switch</property>
	</and>
      </condition>
      <pitch>
	<property>/instrumentation/variometer/te-reading-mps</property>
	<factor>0.075</factor>
	<offset>1.0</offset>
      </pitch>
      <volume>
	<property>/instrumentation/ilec-sc7/volume</property>
	<factor>0.7</factor>
	<max>0.7</max>
      </volume>
      <position>
	<x>-3.35</x>
	<y>0</y>
	<z>0.7</z>
      </position>
      <reference-dist>0.5</reference-dist>
      <max-dist>2.0</max-dist>
    </variometer-up>
    
    <variometer-down>
      <name>variometer-down</name>
      <mode>looped</mode>
      <path>Aircraft/ASK21/Sounds/vario2.wav</path>
      <condition>
	<and>
	  <greater-than>
	    <property>/instrumentation/variometer/te-reading-mps</property>
	    <value>-10.00</value>
	  </greater-than>
	  <and>
	    <less-than>
	      <property>/instrumentation/variometer/te-reading-mps</property>
	      <value>-0.02</value>
	    </less-than>
	  </and>
	  <property>/controls/electric/battery-switch</property>
	</and>
      </condition>
      <pitch>
	<property>/instrumentation/variometer/te-reading-mps</property>
	<factor>0.075</factor>
	<offset>1.0</offset>
      </pitch>
      <volume>
	<property>/instrumentation/ilec-sc7/volume</property>
	<factor>0.7</factor>
	<max>0.7</max>
      </volume>
      <position>
	<x>-3.35</x>
	<y>0</y>
	<z>0.7</z>
      </position>
      <reference-dist>0.5</reference-dist>
      <max-dist>2.0</max-dist>
    </variometer-down>
    
    <winch_tow>
      <name>winch_tow</name>
      <mode>looped</mode>
      <path>Aircraft/ASK21/Sounds/winch_tow.wav</path>
      <condition>
	<and>
	  <not>
	    <property>sim/hitches/winch/open</property>
	  </not>
	  <greater-than>
	    <property>velocities/airspeed-kt</property>
	    <value>2</value>
	  </greater-than>
	</and>
      </condition>
      <volume>
	<property>velocities/airspeed-kt</property>
	<factor>0.01</factor>
	<max>1</max>
      </volume>
    </winch_tow>
    
    <whistle>
      <name>low-speed-howl</name>
      <mode>looped</mode>
      <path>Aircraft/ASK21/Sounds/howl.wav</path>
      <condition>
	<and>
	  <greater-than>
	    <property>orientation/alpha-deg</property>
	    <value>0.8</value>
	  </greater-than>
	  <less-than>
	    <property>orientation/alpha-deg</property>
	    <value>4</value>
	  </less-than>
	  <greater-than>
	    <property>velocities/airspeed-kt</property>
	    <value>35</value>
	  </greater-than>
	  <less-than>
	    <property>velocities/airspeed-kt</property>
	    <value>55</value>
	  </less-than>
	</and>
      </condition>
      <pitch>
	<property>velocities/airspeed-kt</property>
	<factor>0.002</factor>
	<offset>1</offset>
      </pitch>
      <volume>
	<property>orientation/alpha-deg</property>
	<factor>0.02</factor>
	<offset>-0.02</offset>
	<max>0.1</max>
      </volume>
    </whistle>
    
    <whoosh-positive>
      <name>whoosh-positive</name>
      <mode>looped</mode>
      <path>Aircraft/ASK21/Sounds/whoosh-loop.wav</path>
      <condition>
	<greater-than>
	  <property>orientation/pitch-rate-degps</property>
	  <value>15</value>
	</greater-than>
	<and>
	  <greater-than>
	    <property>velocities/airspeed-kt</property>
	    <value>35</value>
	  </greater-than>
	</and>
      </condition>
      <pitch>
	<property>velocities/airspeed-kt</property>
	<factor>0.075</factor>
	<offset>1.0</offset>
      </pitch>
      <volume>
	<property>orientation/pitch-rate-degps</property>
	<factor>0.001</factor>
	<offset>-0.015</offset>
	<max>1</max>
      </volume>
    </whoosh-positive>
    
    <whoosh-negative>
      <name>whoosh-negative</name>
      <mode>looped</mode>
      <path>Aircraft/ASK21/Sounds/whoosh-loop.wav</path>
      <condition>
	<less-than>
	  <property>orientation/pitch-rate-degps</property>
	  <value>-15</value>
	</less-than>
	<and>
	  <greater-than>
	    <property>velocities/airspeed-kt</property>
	    <value>35</value>
	  </greater-than>
	</and>
      </condition>
      <pitch>
	<property>velocities/airspeed-kt</property>
	<factor>0.075</factor>
	<offset>1.0</offset>
      </pitch>
      <volume>
	<property>orientation/pitch-rate-degps</property>
	<factor>-0.001</factor>
	<offset>0.015</offset>
	<max>1</max>
      </volume>
    </whoosh-negative>
    
    <hook_open>
      <name>hook_open</name>
      <mode>once</mode>
      <path>Aircraft/ASK21/Sounds/winch_hook_open.wav</path>
      <condition>
	<property>/sim/hitches/hook-open</property>
      </condition>
      <position> <!--Position of nose hook-->
	<x>-4.2</x>
	<y>0.0</y>
	<z>0.4</z>
      </position>
      <reference-dist>0.5</reference-dist>
      <max-dist>5.0</max-dist>
    </hook_open>
    
    <nose_hook_cable_release>
      <name>nose_hook_cable_release</name>
      <mode>once</mode>
      <path>Aircraft/ASK21/Sounds/winch_release.wav</path>
      <condition>
	<or>
	  <property>/sim/hitches/aerotow/open</property>
	  <property>/sim/hitches/aerotow/broken</property>
	</or>
      </condition>
      <position> <!--Position of nose hook-->
	<x>-4.2</x>
	<y>0.0</y>
	<z>0.4</z>
      </position>
      <reference-dist>5</reference-dist>
      <max-dist>30.0</max-dist>
    </nose_hook_cable_release>
    
    <belly_hook_cable_release>
      <name>belly_hook_cable_release</name>
      <mode>once</mode>
      <path>Aircraft/ASK21/Sounds/winch_release.wav</path>
      <condition>
	<or>
	  <property>/sim/hitches/winch/open</property>
	  <property>/sim/hitches/winch/broken</property>
	</or>
      </condition>
      <position> <!--Position of CG hook-->
	<x>-1.6</x>
	<y>-0.1</y>
	<z>0.23</z>
      </position>
      <reference-dist>5</reference-dist>
      <max-dist>30.0</max-dist>
    </belly_hook_cable_release>
    
    <!-- Stall -->
    
    <stall>
      <name>stall</name>
      <mode>looped</mode>
      <path>Aircraft/ASK21/Sounds/stall.wav</path>
      <condition>
	<and>
	  <property>/instrumentation/alerts/stall</property>
	  <greater-than>
	    <property>/sim/time/elapsed-sec</property>
	    <value>100</value>
	  </greater-than>
	</and>
      </condition>    
      <volume>
	<property>/instrumentation/alerts/stall</property>
	<max>1.0</max>
      </volume>
      <reference-dist>10</reference-dist>
      <max-dist>100</max-dist>
    </stall>
    
    </fx>
  </PropertyList>
  